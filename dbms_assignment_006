Q1="SELECT fname,lname FROM Actor INNER JOIN Cast ON id==pid WHERE mid=12148;"
Q2="SELECT COUNT(id) FROM Actor WHERE fname=='Harrison (I)' AND lname='Ford';"
Q3="SELECT distinct(movieWithcast.pid) FROM (Movie INNER JOIN Cast ON id==mid) as movieWithcast WHERE movieWithcast.name LIKE 'Young Latin Girls%';"
Q4="SELECT COUNT(distinct(movieWithcast.pid)) FROM (Movie INNER JOIN Cast ON id ==mid) as movieWithcast WHERE movieWithcast.year BETWEEN 1990 AND 2000;"
